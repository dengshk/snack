<style type="text/css" >
	#gis-tk-info>ul>li{
		color: #428bca;
		padding: 5px 10px;
		margin-bottom:-10px;
		cursor:pointer;
	}
	#gis-tk-info>ul>li.active_tab{
		background-color: #428bca;
		color: #fff;
	}
